COLUMN TABLE
  "PASSENGERS" ( 
  "PASSENGERID" INTEGER, 
  "NAME" NVARCHAR(256), 
  "ACTIVE" TINYINT, 
  "ADDRESS" NVARCHAR(256), 
  "COUNTRY" NVARCHAR(256), 
  "PASSPORT" NVARCHAR(256), 
  "PASSENGERPRIORITYSTATUS" NVARCHAR(3) NOT NULL, 
  PRIMARY KEY ("PASSENGERID") 
) 
WITH ASSOCIATIONS( 
  JOIN "FLIGHTRESERVATION" AS "FLIGHTRESERVATION" 
ON "PASSENGERID" = "PASSENGERID" 
)