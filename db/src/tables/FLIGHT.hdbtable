COLUMN TABLE "FLIGHT" (
  "FLIGHTID" INTEGER NOT NULL,
  "TAKEOFFTIME" NVARCHAR(8) NOT NULL,
  "TAKEOFFDATE" NVARCHAR(8) NOT NULL,
  "MANDT" NVARCHAR(3) NOT NULL,
  "CARRID" NVARCHAR(3) NOT NULL,
  "CONNID" NVARCHAR(4) NOT NULL,
  "FLDATE" NVARCHAR(8) NOT NULL,
  "SEAT" INTEGER NOT NULL,
  "BOOKINGTIME" NVARCHAR(8) NOT NULL,
  PRIMARY KEY ("FLIGHTID")
) WITH associations(
    join "FLIGHT" AS "FLIGHT" ON "FLIGHTID" = "FLIGHTID"
) unload priority 5 auto merge